@{
    @model List<DockerWithMVC.Entities.Person>
    ViewData["Title"] = "Home Page";
}

<div class="text-center">

    <table>
        <thead className="btn-primary">
            <tr>
                <th>Id</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email Id</th>
                <th>Mobile No</th>
                <th>Address</th>
                <th>PinCode</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.FirstName</td>
                    <td>@item.LastName</td>
                    <td>@item.EmailId</td>
                    <td>@item.MobileNumber</td>
                    <td>@item.Address</td>
                    <td>@item.Pincode</td>
                </tr>
            }
        </tbody>
    </table>
</div>
